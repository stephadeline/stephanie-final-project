{% for member in members %}
<div class="col-md-3">
<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="../assets/img/members/{{member.imagebw}}" alt="{{member.name}}">
  <div class="card-body">
    <h5 class="card-title">{{member.name}}</h5>
    <p class="card-text">{{member.instruments}}</p>
    <a href="{{member.slug}}.html" class="btn btn-primary">Read member bio</a>
  </div>
</div>
</div>
{%endfor%}

