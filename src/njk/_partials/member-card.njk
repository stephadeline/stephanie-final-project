<div class="member-cards">
<div class="container">
<h1 class="text-center">Meet the Members</h1>
<div class ="row">

{% for member in members %}

<div class="col-md-6 col-lg-3">
  <a href="{{member.slug}}.html" class="card-link">
    <div class="card">
      <img class="card-img" src="assets/img/members/{{member.imagebw}}" alt="Black and white photo of {{member.name}}">
      <div class="card-text">
        <h5>{{member.name}}</h5>
        <p>{{member.instruments}}</p>
        <p><em>"{{member.quote}}"</em></p>
      </div>
  </a>
  </div>
</div>
{%endfor%}
</div>
</div>
</div>
